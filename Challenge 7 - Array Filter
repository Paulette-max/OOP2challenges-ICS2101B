public class ArrayFilter {
    public static void main(String[] args) {
        String[] orders = {"B123", "C234", "A345", "C15", "B177", "G3003", "C235", "B179"};

        for (String item: orders) {
            if (item.startsWith("C")) {
                System.out.println(item);
            }
        }
    }
}
